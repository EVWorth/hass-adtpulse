{
  "config": {
    "step": {
      "user": {
        "data": {
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]",
          "fingerprint": "[%key:component::adtpulse::config::step::user::data::fingerprint%]",
          "hostname": "[%key:common::config_flow::data::hostname%]"
        }
      },
      "reauth_confirm": {
        "title": "[%key:common::config_flow::title::reauth%]",
        "description": "The ADT Pulse integration needs to re-authenticate your account"
      }
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "invalid_auth": "[%key:common::config_flow::error::invalid_auth%]",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    },
    "abort": {
      "already_configured": "[%key:common::config_flow::abort::already_configured_device%]",
      "reauth_successful": "[%key:common::config_flow::abort::reauth_successful%]"
    }
  },
  "options": {
    "step": {
      "init": {
        "data": {
          "scan_interval": "[%key:common::config_flow::data::scan_interval%]",
          "relogin_interval": "%[key:component::adtpusle::options::step::init::data::relogin_interval%]",
          "keepalive_interval": "%[key:component::adtpusle::options::step::init::data::keepalive_interval%]"
        }
      }
    }
  }
}
